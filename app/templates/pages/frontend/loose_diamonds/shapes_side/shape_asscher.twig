<div id="normalSideDimensions" style="position: absolute; visibility: visible; vertical-align: top; font-size: 10px; z-index: 50;">
    <div style="position: absolute; left:-16px; bottom:73px; background-color: #FFFFFF; width: 40px;">Table %<br>
        {% if product %}
            {{ product.table }}%
        {% endif %}
        {% if diamond %}
            {{ diamond.table }}%
        {% endif %}
    </div>
    {# <div style="position: absolute; right:81px; top:-4px; background-color: #FFFFFF; width: 40px;">height<br>{{ product.height }}mm</div> #}
    <div style="position: absolute; left:82px; bottom:-10px; background-color: #FFFFFF; width: 40px;">Depth %<br>
        {% if product %}
            {{ product.depth }}%
        {% endif %}
        {% if diamond %}
            {{ diamond.depth }}%
        {% endif %}
    </div>
    {% if diamond.culet %}
    <div style="position: absolute; right: 22px; top: 56px; background-color: #FFFFFF; width: 40px;">Culet<br>
        {{ product.culet.code }}
    </div>{% endif %}
    {# <div style="position: absolute; left: 41px; top: 51px; background-color: #FFFFFF; width: 40px;">Girdle<br></div> #}
</div>