// Search action
$('.c-search-form').submit((event) => {
event.preventDefault();
if ($('.c-search-field').val().length >= 100) {
return false
} else {
return window.location.href = '/search?s=' + $('.c-search-field').val();
}
});

// Search suggestions
$('.c-search-field').autocomplete({
position: {
my: "right top",
at: "right bottom"
},
minLength: 0,
source: function (request, response) {
$('.c-top-search-btn').prop('disabled', false);
$('.c-top-search-btn').removeClass('search-btn-disabled');
$.ajax({
url: "/search/suggestions",
dataType: "json",
data: {
s: request.term
},
success: function (suggestions) {
if (! suggestions.length) {
$('.c-top-search-btn').prop('disabled', true);
$('.c-top-search-btn').addClass('search-btn-disabled');
}
// delegate back to autocomplete
response(suggestions);
}
});
},
focus: function (event, ui) { // prevent value inserted on focus
$('.c-search-field').val(ui.item.category + ': ' + ui.item.title + ' ' + ui.item.description + ((ui.item.vendorEnabled && ui.item.isEnabled) ? '      {{ currency ? currency.sign : "$" }}' + Math.round(ui.item.price {{ currency ? ' * ' ~ currency.rate : '' }}) : ''));
return false;
},
select: function (event, ui) {
$('.c-search-field').val(ui.item.category + ': ' + ui.item.title + ' ' + ui.item.description + ((ui.item.vendorEnabled && ui.item.isEnabled) ? '      {{ currency ? currency.sign : "$" }}' + Math.round(ui.item.price {{ currency ? ' * ' ~ currency.rate : '' }}) : ''));
window.location.href = ui.item.url;
return false;
}
}).autocomplete("instance")._renderItem = function (ul, item) {
if (item.category == 'Diamond') {
let img = '/images/jewelry/default.png';
if (item.image.includes('.jpg') || item.image.includes('.Jpg') || item.image.includes('.png')) {
img = item.image
}
return $('<li class="ui-menu-item">').append(`<div class="ui-menu-item-wrapper search-wrapper"><div class="holder-search-img"><img class="" src="${img}" alt="android-logo-image" /></div>` + '<div>' + item.category + ': ' + item.weight + '<p style="margin-bottom: 0;">SKU: ' + item.sku + '</p>' + item.description + ((item.vendorEnabled && item.isEnabled) ? '<p style="color: green;">' + '      {{ currency ? currency.sign : "$" }}' + Math.round(item.price {{ currency ? ' * ' ~ currency.rate : '' }}) + '</p></div></div>' : '')).appendTo(ul);
} else {
if (item.images.length > 0) {
let image = item.images.find(item => item.type === 'img') || {};
if (image) {
return $('<li class="ui-menu-item">').append(`<div class="ui-menu-item-wrapper search-wrapper"><div class="holder-search-img"><img class="" src="${
image.value
