item.sku
}</span>` + ((item.vendorEnabled && item.isEnabled) ? '{{ currency ? currency.sign : "$" }}' + Math.round(item.price {{ currency ? ' * ' ~ currency.rate : '' }}) + '</p></div>' : '')).appendTo(ul);
